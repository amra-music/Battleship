<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<Pane prefHeight="500.0" prefWidth="700.0" snapToPixel="false" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.startSceneControler">
   <children>
      <ImageView fx:id="startSceneImageView" fitHeight="500.0" fitWidth="700.0" pickOnBounds="true" preserveRatio="true">
         <viewport>
            <Rectangle2D height="500.0" minX="700.0" minY="500.0" width="700.0" />
         </viewport></ImageView>
      <Button fx:id="startButton" alignment="CENTER" layoutX="300.0" layoutY="290.0" mnemonicParsing="false" onMousePressed="#start" prefHeight="30.0" prefWidth="100.0" text="Start">
         <font>
            <Font size="15.0" />
         </font>
      </Button>
      <Button fx:id="optionsButton" layoutX="300.0" layoutY="332.0" mnemonicParsing="false" onMousePressed="#options" prefHeight="30.0" prefWidth="100.0" text="Options">
         <font>
            <Font size="15.0" />
         </font>
      </Button>
      <Button fx:id="exitButton" layoutX="300.0" layoutY="372.0" mnemonicParsing="false" onMousePressed="#exit" prefHeight="30.0" prefWidth="100.0" text="Exit">
         <font>
            <Font size="15.0" />
         </font>
      </Button>
   </children>
</Pane>
