<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<BorderPane prefHeight="289.0" prefWidth="448.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1">
    <top>
        <AnchorPane fx:id="title" onMousePressed="#drag" prefHeight="43.0" prefWidth="1100.0" style="-fx-background-color: #47A3B5;" BorderPane.alignment="CENTER">
            <BorderPane.margin>
                <Insets />
            </BorderPane.margin>
            <cursor>
                <Cursor fx:constant="MOVE" />
            </cursor>
        </AnchorPane>
    </top>
   <center>
      <GridPane alignment="CENTER" BorderPane.alignment="CENTER">
         <columnConstraints>
            <ColumnConstraints halignment="LEFT" hgrow="SOMETIMES" maxWidth="382.0" minWidth="10.0" prefWidth="313.6" />
            <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" maxWidth="325.0" minWidth="10.0" prefWidth="158.2" />
            <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" maxWidth="325.0" minWidth="10.0" prefWidth="162.2" />
         </columnConstraints>
         <rowConstraints>
            <RowConstraints maxHeight="311.2" minHeight="10.0" prefHeight="166.59999999999997" valignment="CENTER" vgrow="SOMETIMES" />
            <RowConstraints maxHeight="194.4" minHeight="10.0" prefHeight="80.6" valignment="TOP" vgrow="SOMETIMES" />
         </rowConstraints>
         <children>
            <ImageView fx:id="endPicture" fitHeight="198.0" fitWidth="263.0" pickOnBounds="true" preserveRatio="true" GridPane.rowSpan="2" />
            <Label fx:id="endTextLabel" textAlignment="CENTER" wrapText="true" GridPane.columnIndex="1" GridPane.columnSpan="2">
               <font>
                  <Font size="22.0" />
               </font>
            </Label>
            <Button fx:id="playAgainButton" mnemonicParsing="false" onMousePressed="#playAgain" prefHeight="30.0" prefWidth="75.0" text="Play again" GridPane.columnIndex="1" GridPane.rowIndex="1" />
            <Button fx:id="exitButton" mnemonicParsing="false" onMousePressed="#exit" prefHeight="30.0" prefWidth="75.0" text="Exit" GridPane.columnIndex="2" GridPane.rowIndex="1" />
         </children>
      </GridPane>
   </center>
</BorderPane>
